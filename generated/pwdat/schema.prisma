generator client {
  provider      = "prisma-client-js"
  output        = "../generated/pwdat"
  binaryTargets = ["native", "windows", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_PWDAT_URL")
}

